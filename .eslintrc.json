{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["standard","prettier"],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "semi": [
            "error",
            "always"
        ],
        "indent": [
            "error",
            4
        ],
        "quotes": [
            "error",
            "single"
        ],
        "no-unused-vars":"warn"
    }
}
